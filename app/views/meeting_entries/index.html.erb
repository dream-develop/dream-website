<p id="notice"><%= notice %></p>

<h1>Meeting Entries</h1>

<table>
  <thead>
    <tr>
      <th>Meeting date</th>
      <th>Family name</th>
      <th>First name</th>
      <th>Family kana</th>
      <th>First kana</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Purpose</th>
      <th>Note</th>
      <th>Agreement</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meeting_entries.each do |meeting_entry| %>
      <tr>
        <td><%= meeting_entry.meeting_name %></td>
        <td><%= meeting_entry.family_name %></td>
        <td><%= meeting_entry.first_name %></td>
        <td><%= meeting_entry.family_kana %></td>
        <td><%= meeting_entry.first_kana %></td>
        <td><%= meeting_entry.phone %></td>
        <td><%= meeting_entry.email %></td>
        <td><%= meeting_entry.purpose %></td>
        <td><%= meeting_entry.note %></td>
        <td><%= meeting_entry.agreement %></td>
        <td><%= link_to 'Show', meeting_entry %></td>
        <td><%= link_to 'Edit', edit_meeting_entry_path(meeting_entry) %></td>
        <td><%= link_to 'Destroy', meeting_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meeting Entry', new_meeting_entry_path %>
